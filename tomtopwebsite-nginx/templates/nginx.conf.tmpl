upstream tomtopwebsite {
{{range getvs "/services/tomtopwebsite-active/*"}}
    server {{.}};
{{end}}
}

server {
    listen       80;

    location / {
        proxy_pass        http://tomtopwebsite;
        proxy_redirect    off;
        proxy_set_header  Host             $host;
        proxy_set_header  X-Real-IP        $remote_addr;
        proxy_set_header  X-Forwarded-For  $proxy_add_x_forwarded_for;
    }
}

